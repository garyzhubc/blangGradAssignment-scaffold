package matchings

model UnivariateLinearRegression {
  param List<RealVar> y ?: #[4,5,6]                      // response variable
  param List<RealVar> x ?: #[1,2,3]                      // explanatory variable
  random RealVar b ?: latentReal                         // coefficient
  
  laws {
    b ~ Normal(0, 1)
    for (int i: 0..< y.size()) {
      y.get(i) | x, i, b ~ Normal(b * x.get(i),1)
    }
  }
}